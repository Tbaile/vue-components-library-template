{
  "name": "template-components-library",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/types/src/main.d.ts",
  "main": "./dist/template-components-library.umd.cjs",
  "module": "./dist/template-components-library.js",
  "exports": {
    ".": {
      "types": "./dist/types/src/main.d.ts",
      "import": "./dist/template-components-library.js",
      "require": "./dist/template-components-library.umd.cjs"
    },
    "./template-components-library.css": "./dist/template-components-library.css"
  },
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@vueuse/core": "^13.0.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@fontsource/poppins": "^5.1.1",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@tailwindcss/vite": "^4.0.0-beta.8",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "^1.1.20",
    "@vitest/ui": "^3.0.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.0",
    "eslint": "^9.14.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^8.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "storybook": "^8.4.7",
    "storybook-addon-pseudo-states": "^4.0.2",
    "tailwindcss": "^4.0.0-beta.8",
    "typescript": "~5.9.0",
    "vite": "^6.0.1",
    "vitest": "^3.0.0",
    "vue-tsc": "^2.1.10"
  }
}
